{
  "name": "styletron-react",
  "version": "6.0.2",
  "description": "React bindings for Styletron",
  "author": "Ryan Tsao <ryan.j.tsao@gmail.com>",
  "repository": "styletron/styletron",
  "sideEffects": false,
  "files": [
    "dist-browser-cjs",
    "dist-browser-esm",
    "dist-node-cjs",
    "dist-node-esm",
    "src",
    "!__tests__"
  ],
  "main": "./dist-node-cjs/index.js",
  "module": "./dist-node-esm/index.js",
  "browser": {
    "./dist-node-cjs/index.js": "./dist-browser-cjs/index.js",
    "./dist-node-esm/index.js": "./dist-browser-esm/index.js"
  },
  "scripts": {
    "build": "cup build --skip-flow && cp ./src/index.js.flow ./dist-node-cjs/",
    "pretest": "cup build-tests",
    "test": "../../node_modules/.bin/unitest --browser=dist-tests/browser.js --node=dist-tests/node.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "styletron-standard": "^3.0.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/webpack-env": "^1.17.0",
    "@babel/preset-react": "^7.17.12",
    "@zxbodya/create-universal-package": "^4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-react": "^7.6.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "unitest": "^2.1.1"
  },
  "license": "MIT"
}
