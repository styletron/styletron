{
  "name": "styletron-engine-monolithic",
  "version": "0.0.6",
  "description": "Universal, high-performance JavaScript styles",
  "author": "Vojtech Miksu <vojtech@miksu.cz>",
  "repository": "styletron/styletron",
  "sideEffects": false,
  "files": [
    "dist",
    "dist-browser-cjs",
    "dist-browser-esm",
    "dist-node-cjs",
    "dist-node-esm",
    "src",
    "!__tests__"
  ],
  "main": "./dist-node-cjs/index.js",
  "module": "./dist-node-esm/index.js",
  "browser": {
    "./dist-node-cjs/index.js": "./dist-browser-cjs/index.js",
    "./dist-node-esm/index.js": "./dist-browser-esm/index.js"
  },
  "scripts": {
    "build": "cup build --skip-flow && cp ./src/index.js.flow ./dist-node-cjs/",
    "pretest": "cup build-tests",
    "test": "unitest --node=dist-tests/node.js --browser=dist-tests/browser.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "inline-style-prefixer": "^5.1.0",
    "styletron-standard": "^3.0.5"
  },
  "devDependencies": {
    "@zxbodya/create-universal-package": "^4.2.0",
    "tape": "4.9.1",
    "unitest": "^2.1.1"
  },
  "license": "MIT"
}
